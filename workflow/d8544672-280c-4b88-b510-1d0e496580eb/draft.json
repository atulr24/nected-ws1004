      "query": {
 "value": "INSERT INTO loan_applications (\n    id,\n    customer_id,\n    monthly_income,\n    credit_score,\n    avg_bank_balance,\n    interest_rate,\n    max_ticket_size,\n    tenure,\n    processing_fee_percent,\n    status,\n    workflow_id,\n    pricing_slab,\n    \"approverId\",\n    \"approverName\",\n    foir_perc,\n    proposed_emi,\n    created_at,\n    edited_at,\n    enquiries_in_last_6m\n    \n) VALUES (\n    {{.Trigger.rowID}},\n    {{.Trigger.customerId}},\n    {{.Trigger.monthlyIncome}},\n    {{.Trigger.creditScore}},\n    {{.Trigger.avgBankBalance}},\n    {{.PricingTierDetails.output[0].InterestRate}},\n    {{.PricingTierDetails.output[0].MaxTicket}},\n    {{.PricingTierDetails.output[0].MaxTenure}},\n    {{.PricingTierDetails.output[0].ProcessingFeePerc}},\n    'Approved',\n    '{{.systemVar.ExecutionId}}',\n    '{{.LoanPricingSlab.output[0].pricing_tier}}',\n    0,\n    'System',\n    {{.Trigger.foirPercent}},\n    {{.Trigger.proposedEmi}},\n    '{{.systemVar.CurrentTime}}',\n    '{{.systemVar.CurrentTime}}',\n    {{.Trigger.enquiriesInLast6m}}\n)\nON CONFLICT (id) \nDO UPDATE SET\n    id = EXCLUDED.id,\n    customer_id = EXCLUDED.customer_id,\n    monthly_income = EXCLUDED.monthly_income,\n    credit_score = EXCLUDED.credit_score,\n    avg_bank_balance = EXCLUDED.avg_bank_balance,\n    interest_rate = EXCLUDED.interest_rate,\n    max_ticket_size = EXCLUDED.max_ticket_size,\n    tenure = EXCLUDED.tenure,\n    processing_fee_percent = EXCLUDED.processing_fee_percent,\n    status = EXCLUDED.status,\n    workflow_id = EXCLUDED.workflow_id,\n    workflow_version = EXCLUDED.workflow_version,\n    pricing_slab = EXCLUDED.pricing_slab,\n    \"approverId\" = {{.Trigger.approverId}},\n    \"approverName\" = '{{.Trigger.approverName}}',\n    foir_perc = EXCLUDED.foir_perc,\n    proposed_emi = EXCLUDED.proposed_emi,\n    created_at = EXCLUDED.created_at,\n    edited_at = EXCLUDED.edited_at,\n    enquiries_in_last_6m= EXCLUDED.enquiries_in_last_6m;",
